            "source": [
                "CREATE TABLE Kunde\n",
                "(\n",
                "  KundeNr VARCHAR(30) NOT NULL,\n",
                "  Strasse VARCHAR(50) NOT NULL,\n",
                "  PLZ CHAR(5) NOT NULL,\n",
                "  Ort VARCHAR(50) NOT NULL,\n",
                "  Firma VARCHAR(50) NOT NULL,\n",
                "  PRIMARY KEY (KundeNr)\n",
                ");\n",
                "CREATE TABLE Monatsrechung\n",
                "(\n",
                "  RechnungsNr INT NOT NULL,\n",
                "  Rechnunsdatum SMALLDATETIME NOT NULL,\n",
                "  KundeNr VARCHAR(30) NOT NULL,\n",
                "  PRIMARY KEY (RechnungsNr),\n",
                "  FOREIGN KEY (KundeNr) REFERENCES Kunde(KundeNr)\n",
                ");\n",
                "CREATE TABLE Fahrzeug\n",
                "(\n",
                "  FahrzeugID INT NOT NULL,\n",
                "  Kennzeichnen VARCHAR(15) NOT NULL,\n",
                "  PRIMARY KEY (FahrzeugID)\n",
                ");\n",
                "CREATE TABLE Fahrzeugtyp\n",
                "(\n",
                "  FahrzeugtypID INT NOT NULL,\n",
                "  Typ VARCHAR(30) NOT NULL,\n",
                "  EURperKM Decimal(9,2) NOT NULL,\n",
                "  EURperStd Decimal(9,2) NOT NULL,\n",
                "  FahrzeugID INT NOT NULL,\n",
                "  FOREIGN KEY (FahrzeugID) REFERENCES Fahrzeug(FahrzeugID)\n",
                ");\n",
                "CREATE TABLE Nutzer\n",
                "(\n",
                "  NutzerID INT NOT NULL,\n",
                "  KundeNr VARCHAR(30) NOT NULL,\n",
                "  PRIMARY KEY (NutzerID),\n",
                "  FOREIGN KEY (KundeNr) REFERENCES Kunde(KundeNr)\n",
                ");\n",
                "CREATE TABLE Parameter\n",
                "(\n",
                "  ParameterID INT NOT NULL,\n",
                "  Parameter VARCHAR(30) NOT NULL,\n",
                "  Wert DECIMEL(9,2) NOT NULL,\n",
                "  PRIMARY KEY (ParameterID)\n",
                ");\n",
                "CREATE TABLE Fahr\n",
                "(\n",
                "  Abholung SMALLDATETIME NOT NULL,\n",
                "  Rueckgabe SMALLDATETIME NOT NULL,\n",
                "  StartKm DECIMAL(9,3) NOT NULL,\n",
                "  EndKm DECIMAL(9,3) NOT NULL,\n",
                "  NutzerID INT NOT NULL,\n",
                "  FahrzeugID INT NOT NULL,\n",
                "  PRIMARY KEY (NutzerID, FahrzeugID),\n",
                "  FOREIGN KEY (NutzerID) REFERENCES Nutzer(NutzerID),\n",
                "  FOREIGN KEY (FahrzeugID) REFERENCES Fahrzeug(FahrzeugID)\n",
